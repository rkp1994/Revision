

//finding the length of the string

#include<stdio.h>
#include<string.h>

int main()
{
    char a[100];
    int length;
    
    printf("Enter the string :-\n");
    gets(a);
    puts(a);
    
    
    length=strlen(a);
    
    printf("The Length of the string is:- %d \n",length);
    
    return 0;
}

